{
  "$schema": "https://raw.githubusercontent.com/spetersenms/AL-Go/refactor/Actions/.Modules/settings.schema.json",
  "type": "PTE",
  "templateUrl": "https://github.com/spetersenms/AL-Go@refactor",
  "generateDependencyArtifact": true,
  "incrementalBuilds": {
    "onPush": true,
    "onPull_Request": true,
    "mode": "modifiedApps"
  },
  "DeployToProduction": {
    "buildMode": "Clean"
  },
  "templateSha": "bf8d0cdf10a350431f64171687276d73d65cfe9a",
  "enableCodeCop": true,
  "enableUICop": true,
  "rulesetFile": "../rulesets/ruleset.json",
  "trackCodeCopIssuesInGitHub": true,
  "useProjectDependencies": true,
  "buildModes": [
    "Clean"
  ],
  "appDependencyProbingPaths": [
    {
      "repo": "https://github.com/spetersenmsorg/Al-Go_MultiProjectTest",
      "version": "latest",
      "release_status": "release",
      "projects": "Project2"
    }
  ]
}
