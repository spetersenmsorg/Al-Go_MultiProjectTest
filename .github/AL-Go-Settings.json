  "type": "PTE",
  "templateUrl": "https://github.com/spetersenms/AL-Go@refactor",
  "generateDependencyArtifact": true,
  "incrementalBuilds": {
    "onPush": true,
    "onPull_Request": true,
    "mode": "modifiedApps"
  },
  "DeployToProduction": {
    "buildMode": "Clean",
    "environmentName": "AL-Go-Demo-Test"
  },
  "templateSha": "7b602a8379f7c9eb25dbe4592c721bb477d942d0",
  "enableCodeCop": true,
  "enableUICop": true,
  "rulesetFile": "../rulesets/ruleset.json",
  "trackCodeCopIssuesInGitHub": true,
  "useProjectDependencies": true,
  "buildModes": [
    "Clean"
  ],
  "appDependencyProbingPaths": [
    {
      "repo": "https://github.com/spetersenmsorg/Al-Go_MultiProjectTest",
      "version": "latest",
      "release_status": "release",
      "projects": "Project2"
